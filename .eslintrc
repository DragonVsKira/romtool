{
  "extends": ["airbnb-base", "prettier"],
  "rules": {
    "linebreak-style": 0,
    "no-console": 0,
    "global-require": 0,
    "no-unused-expressions": [
      2,
      { "allowShortCircuit": true, "allowTernary": true }
    ],
    "no-use-before-define": 0,
    "no-plusplus": 0,
    "no-return-assign": 0,
    "consistent-return": 0,
    "no-await-in-loop": 0
  }
}
